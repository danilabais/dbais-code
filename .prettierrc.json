{
  "quotes": ["error", "double"],
  "import/prefer-default-export": "off",
  "linebreak-style": "off",
  "prettier/prettier": [
    "error",
    {
      "endOfLine": "auto",
      "htmlAttributeWrap": "always",
      "htmlButtonContentWrap": true
    }
  ],
  "import/order": [
    "warn",
    {
      "groups": [
        "builtin",
        "external",
        "internal",
        "parent",
        "sibling",
        "index"
      ],
      "newlines-between": "always"
    }
  ],
  "import/newline-after-import": [
    "warn",
    {
      "count": 1
    }
  ],
  "no-unused-vars": "warn",
  "no-undef": "warn",
  "max-lines": [
    "warn",
    {
      "max": 500,
      "skipBlankLines": true,
      "skipComments": false
    }
  ],
  "padding-line-between-statements": [
    "warn",
    {
      "blankLine": "always",
      "prev": "*",
      "next": "return"
    }
  ]
}
